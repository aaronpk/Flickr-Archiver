<div class="container">

<div class="row">
<div class="span16">

  <h2>Recent Photos</h2>

  <ul class="media-grid photos">
    <% @photos.each do |item| puts item['photo'].to_hash %>
      <li class="<%= item['class'] %>"><a href=""><img class="thumbnail" src="<%= item['photo'].url_sq %>" /></a></li>
    <% end %>
  </ul>

</div><!-- span16 -->
</div><!-- row -->

</div><!-- container -->